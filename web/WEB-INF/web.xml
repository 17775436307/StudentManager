<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">
    <welcome-file-list>
        <welcome-file>welcome.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <description></description>
        <display-name>CheckCodeServlet</display-name>
        <servlet-name>CheckCodeServlet</servlet-name>
        <servlet-class>com.example.controller.CheckCodeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckCodeServlet</servlet-name>
        <url-pattern>/CheckCodeServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>CheckCodeServlet2</display-name>
        <servlet-name>CheckCodeServlet2</servlet-name>
        <servlet-class>com.example.controller.CheckCodeServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckCodeServlet2</servlet-name>
        <url-pattern>/CheckCodeServlet2</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>UserServlet</display-name>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>com.example.controller.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/UserServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>StudentServlet</display-name>
        <servlet-name>StudentServlet</servlet-name>
        <servlet-class>com.example.controller.StudentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentServlet</servlet-name>
        <url-pattern>/StudentServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>TeacherServlet</display-name>
        <servlet-name>TeacherServlet</servlet-name>
        <servlet-class>com.example.controller.TeacherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherServlet</servlet-name>
        <url-pattern>/TeacherServlet</url-pattern>
    </servlet-mapping>
</web-app>
